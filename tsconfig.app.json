{
    "extends": "./tsconfig.base.json",
    "compilerOptions": {
      "outDir": "./out-tsc/app",
      "types": []
    },
    "angularCompilerOptions": {
      "preserveWhitespaces": true,
      "enableIvy": true,
      "allowEmptyCodegenFiles": true
    }
  ,
    "files": [
      "src/main.ts",
      "src/polyfills.ts"
    ],
    "include": [
      "src/**/*.d.ts"
    ]
  }