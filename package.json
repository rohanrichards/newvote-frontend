{
  "name": "newvote-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod --base-href /",
    "build-staging": "npm run env -s && ng build --configuration=stage --base-href /",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js --disable-host-check",
    "serve:sw": "npm run build -s && npx http-server ./www -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version recaptcha_sitekey",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.2.15",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/flex-layout": "7.0.0-beta.24",
    "@angular/forms": "^7.2.15",
    "@angular/http": "^7.2.15",
    "@angular/material": "^7.0.1",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@angular/service-worker": "^7.2.15",
    "@auth0/angular-jwt": "^2.0.0",
    "@cloudinary/angular-5.x": "^1.0.2",
    "@datorama/akita": "^4.10.2",
    "@ionic-native/core": "^5.12.0",
    "@ionic-native/splash-screen": "^5.12.0",
    "@ionic-native/status-bar": "^5.12.0",
    "@ngx-share/core": "^7.1.2",
    "@ngx-translate/core": "^10.0.1",
    "angular-font-awesome": "^3.1.2",
    "angulartics2": "^6.3.1",
    "chart.js": "^2.7.3",
    "cloudinary-core": "^2.5.0",
    "core-js": "^3.2.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.15",
    "material-design-icons-iconfont": "^5.0.1",
    "ng-lazyload-image": "^6.1.0",
    "ng-recaptcha": "^5.0.0",
    "ng2-file-upload": "^1.3.0",
    "ngx-clipboard": "^12.2.0",
    "ngx-cookie-service": "^2.2.0",
    "ngx-joyride": "^2.2.10",
    "ngx-material-file-input": "^1.1.0",
    "ngx-quill": "^7.1.2",
    "ngx-socket-io": "^3.0.1",
    "ngx-swiper-wrapper": "^7.2.1",
    "quill": "^1.3.6",
    "rxjs": "^6.5.2",
    "zone.js": "^0.10.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.2",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@datorama/akita-ngdevtools": "^3.0.2",
    "@ngx-rocket/scripts": "^3.0.0",
    "@types/jasmine": "^2.8.7",
    "@types/jasminewd2": "^2.0.2",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.9",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "2.2.0",
    "codelyzer": "^4.4.4",
    "eslint": "^6.4.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "hads": "^1.7.0",
    "htmlhint": "^0.10.0",
    "https-proxy-agent": "^2.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.2",
    "karma-jasmine": "^1.1.2",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "~5.4.0",
    "puppeteer": "^1.4.0",
    "stylelint": "~9.6.0",
    "stylelint-config-recommended-scss": "~3.2.0",
    "stylelint-config-standard": "~18.2.0",
    "stylelint-scss": "~3.3.0",
    "ts-node": "~8.3.0",
    "typescript": "3.2.4"
  }
}
