{
  "name": "newvote-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod --base-href /",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js --disable-host-check",
    "serve:sw": "npm run build -s && npx http-server ./www -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "cordova:prepare": "ngx-scripts cordova prepare",
    "cordova:run": "ngx-scripts cordova run",
    "cordova:build": "ngx-scripts cordova build --device --release --copy dist",
    "cordova:clean": "ngx-scripts clean",
    "cordova": "cordova",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/cdk": "^7.1.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/material": "^7.0.1",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@angular/service-worker": "^7.0.0",
    "@auth0/angular-jwt": "^2.0.0",
    "@cloudinary/angular-5.x": "^1.0.2",
    "@ionic-native/core": "^4.5.3",
    "@ionic-native/splash-screen": "^4.5.3",
    "@ionic-native/status-bar": "^4.5.3",
    "@ngx-share/core": "^7.0.0",
    "@ngx-translate/core": "^10.0.1",
    "angular-font-awesome": "^3.1.2",
    "angulartics2": "^6.0.0",
    "chart.js": "^2.7.3",
    "cloudinary-core": "^2.5.0",
    "cordova-android": "^7.0.0",
    "cordova-custom-config": "^5.0.2",
    "cordova-ios": "^4.5.4",
    "cordova-plugin-device": "^2.0.1",
    "cordova-plugin-ionic-keyboard": "^2.1.2",
    "cordova-plugin-ionic-webview": "^2.0.0",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.1",
    "cordova-plugin-whitelist": "^1.3.3",
    "core-js": "^2.5.6",
    "font-awesome": "^4.7.0",
    "gulp": "^4.0.2",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.10",
    "material-design-icons-iconfont": "^3.0.3",
    "ng-recaptcha": "^4.2.1",
    "ng2-charts": "^1.6.0",
    "ng2-file-upload": "^1.3.0",
    "ngx-clipboard": "^12.0.0",
    "ngx-material-file-input": "^0.3.0",
    "ngx-quill": "^4.1.0",
    "ngx-swiper-wrapper": "^7.2.1",
    "quill": "^1.3.6",
    "rxjs": "^6.3.1",
    "rxjs-compat": "^6.1.0",
    "zone.js": "^0.8.26"
